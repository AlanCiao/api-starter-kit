{
  "name": "qijitech/api-starter-kit",
  "type": "library",
  "description": "api starter kit",
  "keywords": [
    "laravel 5.2",
    "lumen 5.2",
    "api",
    "starter kit"
  ],
  "homepage": "https://github.com/qijitech/api-starter-kit",
  "authors": [
    {
      "name": "YuGang Yang",
      "email": "smartydroid@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.2.*",
    "illuminate/database": "5.2.*",
    "illuminate/pagination": "5.2.*",
    "illuminate/validation": "5.2.*",
    "dingo/api": "dev-master",
    "tymon/jwt-auth": "dev-develop",
    "mnabialek/laravel-sql-logger": "dev-master"
  },
  "require-dev": {
    "illuminate/foundation": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Api\\StarterKit\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
